    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.2.17",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-table-next": "^1.1.4",
    "react-bootstrap-table2-editor": "^1.0.1",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.5.2",
    "react-moment": "^0.8.1",
